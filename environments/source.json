{
  "json_class": "Chef::Environments",
  "description": "A default environment for testing purposes",
  "chef_type": "environment",
  "default_attributes": {
    "gibbon": {
      "version": "v19.0.00",
      "database_host": "localhost",
      "username": "gibbon",
      "password": "password",
      "database_name": "gibbon"
    },
    "php": {
      "install_method": "source",
      "configure_options": [
        "--with-gd",
        "--with-pdo-mysql",
        "--with-curl",
        "--with-zip"
      ]
    },
    "apache":{
      "modules": [
        "rewrite"
      ]
    }
  },
  "run_list": [
    "recipe[apache_install]"
  ]
}
