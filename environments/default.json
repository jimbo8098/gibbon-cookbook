{
  "json_class": "Chef::Environments",
  "description": "A default environment for testing purposes",
  "chef_type": "environment",
  "default_attributes": {
    "gibbon": {
      "version": "v19.0.00",
      "site_name": "gibbon.localhost",
      "gibbon_dir": "/var/www/gibbon",
      "port": "80",
      "ip": "*",
      "database": {
        "host": "127.0.0.1",
        "username": "gibbon_user",
        "password": "password",
        "name": "gibbon",
        "port": 3306
      }
    },
    "mariadb": {
      "server_root_password": "",
      "mysqld": {
        "port": 3306
      }
    },
    "php": {
      "install_method": "package",
      "pear_setup": false
    },
    "apache":{
      "modules": [
        "rewrite"
      ]
    }
  }
}
